{
  "name": "pintora-diagram-pie-chart",
  "version": "0.0.1",
  "description": "An example pie diagram plugin for pintora.js ",
  "module": "dist/pintora-diagram-pie-chart.esm.js",
  "umd": "dist/pintora-diagram-pie-chart.umd.js",
  "source": "src/index.ts",
  "scripts": {
    "watch": "microbundle --no-compress --globals @pintora/standalone=pintora --external @pintora/* -f umd --watch",
    "dev": "node scripts/dev.js",
    "build": "microbundle --globals @pintora/standalone=pintora --external @pintora/* -f umd,esm",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pintora-diagram"
  ],
  "author": "hikerpig",
  "license": "MIT",
  "devDependencies": {
    "@pintora/standalone": "^0.1.0-alpha.21",
    "live-server": "^1.2.1",
    "microbundle": "^0.13.3",
    "typescript": "^4.3.5"
  }
}
